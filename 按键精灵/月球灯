Function key2(fl, del,offset)
	Select Case fl
		Case size1
			a = inchIn1 + offset
			a=CStr(a)
			Call cutStr(a,del)
		Case size2
			a = inchIn2 + offset
			a=CStr(a)
			Call cutStr(a,del)
		Case size3
			a = inchIn3 + offset
			a=CStr(a)
			Call cutStr(a,del)
		Case size4
			a = inchIn4 + offset
			a=CStr(a)
			Call cutStr(a,del)
		Case size5
			a = inchIn5 + offset
			a=CStr(a)
			Call cutStr(a,del)
		Case size6
			a = inchIn6 + offset
			a=CStr(a)
			Call cutStr(a,del)
		Case size7
			a = inchIn7 + offset
			a=CStr(a)
			Call cutStr(a,del)
		Case size8
			a = inchIn8 + offset
			a=CStr(a)
			Call cutStr(a,del)
	End Select
	
End Function
