  Sub Thread2()
  	TracePrint "进程2开始工作 "
     Delay softDelay*1000
     TracePrint "我是进程2 "
      	//If Plugin.Color.IsDisplayDead(310,182,345,217,4000) Then 
			//TracePrint  "卡死了"
			While 1
			iDead = Plugin.Color.IsDisplayDead(102, 44, 540, 225 , stuckS*1000)
			//TracePrint iDead
				If iDead = True Then 
					TracePrint "软件卡死了"
					TracePrint "关闭软件"
					//下面这句在屏幕区域范围内截图保存到("C:\PIC.bmp")里
					randStr = Lib.算法.随机字符串(3)
					TracePrint savePath&fileStr&"-"&"出错"&randStr&".jpg"
					Plugin.Pic.PrintScreen 0,0,1920,1080,savePath&"\"&fileStr&"-"&"出错"&randStr&".jpg"
					Delay 1000
  					pathArr = split(path, "\")
					Call Lib.系统.结束进程(pathArr(Ubound(pathArr)))
  					Delay 2000
  					KeyPress "Enter", 1
  					Delay 1000
  					RestartScript
				End If
			Wend
  End Sub 
 Sub Thread3()
 	While 1
 		TracePrint "干扰快速保存"
 		If NUMLOCK Then 
 			KeyPress "Num Lock", 1
 		Delay 500
	
		KeyPress "Num Lock", 1
 		End If
 		
		Delay  10000
 	Wend
 End Sub





