		deleteI=0
		While deleteI = 0
  			TracePrint "1"
  			Delay 500
  			FindPic 0,0,1920,1080,"Attachment:\隐藏.bmp",0.9,intX,intY
			If intX > 0 And intY > 0 Then 
			MoveTo intX + 10, intY + 10
			Delay del
			LeftClick 1
			deleteI=1
  			
 
  			
			End If
			KeyPress "Space", 1
  		Wend
		//Call fpc(0, 0, 1920, 1080, "隐藏.bmp", 0.9, 5, 5, del)
		
