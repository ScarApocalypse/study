Private Declare Function GetKeyState Lib"user32"(ByVal nVirtKey As Long) As Integer
If GetKeyState(20) = 0 Then KeyPress 20, 1//检测大写键是否打开，未打开则打开大写键 

  Dimenv fileStr
  Dimenv ID1
  Dimenv ID2
  Dimenv ID3
  DimEnv NUMLOCK
  ID1=BeginThread(Thread1())
  If stuckOpen Then 
  	 ID2=BeginThread(Thread2())
  	 TracePrint "卡屏判断打开"
  End If
  //ID3=BeginThread(Thread3())
  //下面是主线程缓冲，必须加这个无限循环
  While 1
      Delay 1000
  Wend 

  Sub Thread1
  		NUMLOCK=0
		If isOpen Then 
		 	Form1.ComboBox1.ListIndex=0
