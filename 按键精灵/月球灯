Function key3(fl, del)
	Select Case fl
		Case size1
			Call cutStr(xyz1,del)
		Case size2
			Call cutStr(xyz2,del)
		Case size3
			Call cutStr(xyz3,del)
		Case size4
			Call cutStr(xyz4,del)
		Case size5
			Call cutStr(xyz5,del)
		Case size6
			Call cutStr(xyz6,del)
		Case size7
			Call cutStr(xyz7,del)
		Case size8
			Call cutStr(xyz8,del)
	End Select
	
End Function
 
