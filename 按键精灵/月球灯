Function fpc(x1 ,y1 ,m1 ,n1 ,picPath,sim,offsetX,offsetY,sdel)
	UserVar intX=0"1"
	UserVar intY=0"1"
	Do Until intX > 1 And intY > 1
		FindPic x1, y1, m1, n1, "Attachment:\" & picPath, sim, intX, intY
		
	Loop  
	//Delay del
	//TracePrint intX&","&intY
	//TracePrint offsetX&","&offsetY
	intX = intX + offsetX
	intY = intY + offsetY
	
	//TracePrint intX&","&intY
	MoveTo intX, intY
	Delay sdel
	LeftClick 1
	Delay sdel
End Function
Function nfpc(x1 ,y1 ,m1 ,n1 ,picPath,sim,sdel)
	Delay sdel
	UserVar intX=0"1"
	UserVar intY=0"1"
	Do Until intX < 0 And intY < 0
		FindPic x1, y1, m1, n1, "Attachment:\" & picPath, sim, intX, intY
		//TracePrint intX&","&intY
		//LeftClick 1
	Loop
	//Delay del
	//Delay sdel
End Function
Function key1(fl,del)//外圈大小按键
	Select Case fl
Case size1//8cm
	/*Delay  del
    KeyPress "3", 1
    Delay  del
    KeyPress ".", 1
     Delay  del
    KeyPress "1", 1
     Delay  del
    KeyPress "1", 1
    Delay  del
    KeyPress "", 1*/
    Call cutStr(inchOut1,del)
    
Case size2//9cm
	/*Delay  del
    KeyPress "3", 1
     Delay  del
    KeyPress ".", 1
     Delay  del
    KeyPress "5", 1
     Delay  del
    KeyPress "4",1*/
     Call cutStr(inchOut2,del)
Case size3//10cm
	/*Delay  del
    KeyPress "3", 1
     Delay  del
    KeyPress ".", 1
     Delay  del
    KeyPress "9", 1
     Delay del
    KeyPress "0", 1*/
    Call cutStr(inchOut3,del)
Case size4//12cm
	/*Delay  del
	KeyPress "4", 1
	 Delay  del
    KeyPress ".", 1
     Delay  del
    KeyPress "7", 1
     Delay del
    KeyPress "0", 1*/
     Call cutStr(inchOut4,del)
Case size5//15cm
	/*Delay  del	
     KeyPress "5", 1
      Delay  del
    KeyPress ".", 1
     Delay  del
    KeyPress "8", 1
     Delay  del
    KeyPress "7", 1*/
     Call cutStr(inchOut5,del)
Case size6//18cm
	/*Delay  del
     KeyPress "7", 1
      Delay  del
    KeyPress ".", 1
     Delay  del
    KeyPress "0", 1
     Delay  del
    KeyPress "5", 1*/
     Call cutStr(inchOut6,del)
Case size7//20cm
	/*Delay  del
     KeyPress "7", 1
     Delay  del
    KeyPress ".", 1
    Delay  del
    KeyPress "6", 1
    Delay  del
    KeyPress "8", 1*/
     Call cutStr(inchOut7,del)
Case size8//22cm
	/*Delay  del
    KeyPress "8", 1
    Delay  del
    KeyPress ".", 1
    Delay  del
    KeyPress "5", 1
    Delay  del
    KeyPress "0", 1*/
     Call cutStr(inchOut8,del)       
       
End Select	
End Function
