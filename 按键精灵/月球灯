  				TracePrint "关闭软件"
  				pathArr = split(path, "\")
				Call Lib.系统.结束进程(pathArr(Ubound(pathArr)))
  				Delay 2000
  				RestartScript
  			End If
  			//获取文件夹字符串并剪切,确定保存stl文件名
  			fileNameArr = Lib.文件.遍历指定目录下所有文件名(filePath)
			TracePrint fileNameArr//第一个文件的名字
			
			
			TracePrint filePath
			fileStr = fileNameArr(0)
			strArr = split(fileStr, "-")
			picStr=split(strArr(UBound(strArr)),".")(0)
			TracePrint "这是一个"&picStr&"的球"
			saveName = split(fileStr, ".")(0) & ".stl"
			TracePrint saveName
			psdPath=filePath & "\" & fileStr
			stlPath = savePath& "\" & saveName
			movePath = savePath & "\" & fileStr
			TracePrint filePath
			TracePrint savePath
			TracePrint psdPath
			TracePrint stlPath
			TracePrint movePath
			TracePrint fileStr
			TracePrint flag
			Select Case fileStr
				Case flag
				 StopThread ID2	
				
				 If isShutDown Then 
				 	RunApp("cmd.exe") 
					SayString("shutdown.exe -s -t 60")
					KeyPress"Enter",1 
					Call Lib.系统.结束进程("cmd.exe")
				 End If
				 MsgBox ("图片处理完毕")
				 ExitScript
			End Select
			
		
			//ExitScript
			//开始画球
			flag=fileStr
			MoveTo 686, 534//移动到画布中心
			delay del	
			RightDown 1
			MoveR 100, 100
